{
  "name": "strava-club-workers",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "wrangler dev --config ../wrangler.workers.toml",
    "build": "tsc --noEmit",
    "deploy": "wrangler deploy --config ../wrangler.workers.toml",
    "deploy:dev": "wrangler deploy --config ../wrangler.workers.dev.toml",
    "tail": "wrangler tail --config ../wrangler.workers.toml",
    "tail:dev": "wrangler tail --config ../wrangler.workers.dev.toml",
    "migrate": "wrangler d1 migrations apply strava-club-db --local",
    "migrate:remote": "wrangler d1 migrations apply strava-club-db --remote"
  },
  "dependencies": {},
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "wrangler": "^4.47.0",
    "typescript": "^5.6.3"
  }
}
